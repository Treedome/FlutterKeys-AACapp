                                            .frame(width: 50, height: 44)
                                            .onTapGesture(perform: {
                                                Settings.toggle()
                                            })
                                    }
                                    //edit
                                    ZStack{
                                        Rectangle()
                                            .foregroundStyle(Color.black)
                                            .frame(width: 50, height: 42)
                                            .cornerRadius(5)
                                            .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                        
                                        Image(.edit)
                                            .resizable()
                                            .frame(width: 48, height: 40)
