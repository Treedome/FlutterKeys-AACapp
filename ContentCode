                                    .onTapGesture(perform: {dismissKeyboard(); falseFolders(); Nouns = true})
                                Text("Nouns").offset(x:0, y:23).font(.system(size: 12, weight: .semibold))
                            }
                            FSpace
                    //Verbs
                            ZStack{
                                folder
                                Image(.verbs).resizable().frame(width: 40, height: 40).offset(x:0, y:-6)
                                    .onTapGesture(perform: {dismissKeyboard(); falseFolders(); Verbs = true})
                                Text("Verbs").offset(x:0, y:23).font(.system(size: 12, weight: .semibold))
                            }
                       
                            Spacer()
                                .frame(width: 90)
                      
                            
                            
                    //Pinned
                            ZStack{
                                Rectangle()
                                    .frame(width: 60, height: 65)
                                    .foregroundStyle(Color.white)
                                    .cornerRadius(5)
                                    .shadow(color: Color.black.opacity(1), radius: 2, y: 2)
                                Image(.pinned)
                                    .resizable()
                                    .frame(width: 55, height: 60)
                                    .onTapGesture(perform: {dismissKeyboard(); falseFolders()})
                            }
                            
                            FSpace
                    //Store
                            ZStack{
                                Rectangle()
                                    .frame(width: 75, height: 55)
                                    .foregroundStyle(Color.white)
                                    .cornerRadius(5)
                                    .shadow(color: Color.black.opacity(1), radius: 2, y: 2)
                                Image(.store)
                                    .resizable()
                                    .frame(width: 75, height: 50)
                            }
                            
                            FSpace
