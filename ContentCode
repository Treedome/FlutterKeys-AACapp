import Foundation
import SwiftUI
import AVFoundation


struct ContentView: View {
    
    @State var synth = AVSpeechSynthesizer()
    @State var message: String = ""
    @State var rate1: Float = 1
    @State var pitch1: Float = 0
    @State var voiceIndex = 0
    
    @State private var submittedTexts: [String] = []
    
    @State var Expand = false
    @State var Settings = false
    
    @State var Verbs = false
    @State var Pronouns = false
    @State var Nouns = false
    @State var Adj = false
    
    @State var History1 = false
    
    @State var Social = false
    @State var MyTopics = false
    @State var Adverbs = false
    @State var LittleWords = false
    
    @State var PausePlay = true
    
    @State private var ExpandedSize: CGFloat = 20
    
    @State private var CurrentlySpeaking = false
    
    func falseFolders() { LittleWords = false; Pronouns = false; Nouns = false; Verbs = false; Adj = false; Social = false; MyTopics = false; Adverbs = false; Settings = false; History1 = false; Expand = false}
    
    
    func dismissKeyboard(){
        UIApplication.shared.sendAction(#selector(UIResponder.resignFirstResponder), to: nil, from: nil, for: nil)
        

    
    }
    
    var body: some View {
        
     
        let DarkGray = Color(hue: 0.1, saturation: 0.0, brightness: 0.45)
        let LightGray = Color(hue: 0.1, saturation: 0.0, brightness: 0.77)
        let Tile = Rectangle()
            .foregroundStyle(LightGray)
            .frame(width: 87, height: 75)
            .cornerRadius(5)
            .shadow(color: DarkGray.opacity(1), radius: 0.5, y: 2)
        
        let Voices: [AVSpeechSynthesisVoice] = AVSpeechSynthesisVoice.speechVoices()
         
        
        ZStack{
            VStack{
    //message window row starts
                ZStack{
                    Rectangle ()
                        .foregroundStyle(DarkGray)
                        .frame(height: 155)
    
                    HStack{
                        Spacer()
                            .frame(width:15)
                        
    //Left message window
                        VStack{
                            Spacer()
                                .frame(height: 5)
                    //settings
                            ZStack{
                                Rectangle()
                                    .foregroundStyle(Color.black)
                                    .frame(width: 50, height: 42)
                                    .cornerRadius(5)
                                    .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                Image(.settings1)
                                    .resizable()
                                    .frame(width: 50, height: 44)
                                    .onTapGesture(perform: {
                                        dismissKeyboard(); falseFolders(); Settings.toggle()
                                    })
                            }
                            Spacer()
                                .frame(height: 5)
                    // create
                            ZStack{
                                Rectangle()
                                    .foregroundStyle(Color.black)
                                    .frame(width: 50, height: 42)
                                    .cornerRadius(5)
                                    .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                Image(.create)
                                    .resizable()
                                    .frame(width: 50, height: 44)
                            }
                            Spacer()
                                .frame(height: 5)
                    // expand
                            ZStack{
                                Rectangle ()
                                    .foregroundStyle(Color.black)
                                    .frame(width: 65, height: 42)
                                    .cornerRadius(5)
                                    .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                Image(.expand)
                                    .resizable()
                                    .frame(width: 50, height: 44)
                                    .onTapGesture(perform: {
                                        dismissKeyboard(); falseFolders(); Expand.toggle()
                                    })
                            }
                            Spacer()
                                .frame(height: 5)
                        }
                       
                        
    //message window
                        
                        TextField(
                            "Message Window...", text: $message, axis: .vertical
                        )
                        .textInputAutocapitalization(.sentences)
                        .frame(alignment: .top)
                        .autocorrectionDisabled(false)
                        .font(.system(size: 18))
                        .fontWeight(.medium)
                        .padding(.init(top: 10, leading: 15, bottom: 10, trailing: 15))
                        .lineLimit(5...5)
                        .background(Color.white)
                        .onTapGesture(perform: {falseFolders()})
                        .submitLabel(.done)
                        .onAppear{
                            
                        }
                        .onSubmit {
                            submittedTexts.append(message)
                            AVSpeechUtterance(string: message).voice = AVSpeechSynthesisVoice.speechVoices()[voiceIndex]
                            AVSpeechUtterance(string: message).rate = rate1
                            AVSpeechUtterance(string: message).pitchMultiplier = pitch1
                            synth.speak(AVSpeechUtterance(string: message))
                            
                        }
                        
    //Right message window
                        VStack{
                        
                            HStack{
                        //Clear
                                ZStack{
                                    Rectangle()
                                        .foregroundStyle(Color.black)
                                        .frame(width:65, height: 50)
                                        .cornerRadius(5)
                                        .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                    Image(.clear)
                                        .resizable()
                                        .frame(width: 60, height: 40)
                                        .onTapGesture(perform: { message = ""})
                                }
                                Spacer()
                                    .frame(width: 15)
                       //Search
                                ZStack{
                                    Rectangle()
                                        .foregroundStyle(Color.black)
                                        .frame(width: 65, height: 50)
                                        .cornerRadius(5)
                                        .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                    Image(.search)
                                        .resizable()
                                    .frame(width: 49, height: 50)}
                                    }
                            
                            HStack{
                      //undo
                                ZStack{
                                    Rectangle()
                                        .foregroundStyle(Color.black)
                                        .frame(width: 48, height: 48)
                                        .cornerRadius(5)
                                        .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                    //undo
                                    Image(.undo)
                                        .resizable()
                                        .frame(width: 50, height: 50)
                                }
                                Spacer()
                                    .frame(width: 10)
                    //play/pause
                                ZStack{
                                    Rectangle()
                                        .foregroundStyle(Color.black)
                                        .frame(width: 48, height: 48)
                                        .cornerRadius(5)
                                        .shadow(color: LightGray.opacity(1), radius: 0.5, y: 2)
                                    
                                    if PausePlay {
                                        Image(.play)
                                            .resizable()
                                            .frame(width: 45, height: 45)
                                            .onTapGesture(perform: {
                                                PausePlay = false;
                                                    submittedTexts.append(message)
                                                    synth.speak(AVSpeechUtterance(string: message))
                                                    AVSpeechUtterance(string: message).rate = rate1
                                                    AVSpeechUtterance(string: message).pitchMultiplier = pitch1
                                            })
                                        Image(.pause)
                                            .resizable()
                                            .frame(width: 45, height: 45)
                                            .hidden()
                                            
                                    }
                                    if PausePlay == false {
                                        Image(.play)
                                            .resizable()
                                            .frame(width: 45, height: 45)
                                            .hidden()
                                            
                                        Image(.pause)
                                            .resizable()
                                            .frame(width: 45, height: 45)
                                            .onTapGesture(perform: {
                                                PausePlay = true; synth.pauseSpeaking(at: .word
                                                )
                                            })
                                            
                                    }
                                }
                                Spacer()
                                    .frame(width: 10)
