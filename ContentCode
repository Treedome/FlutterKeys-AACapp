                                ForEach((0..<submittedTexts.count).reversed(), id: \.self) { index in
                                    let Submittedtext = submittedTexts[index]
                                    
                                        Button(action: {
                                            message += Submittedtext
                                    
                                        }) {
                                              
                                                Text(Submittedtext)
                                                    .frame(width: 700)
