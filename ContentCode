                            ZStack{
                                folder
                                Image(.littleWords).resizable().frame(width: 70, height: 70).offset(x:0, y:-6)
                                    .onTapGesture(perform: {dismissKeyboard();falseFolders(); LittleWords = true})
                                Text("Little Words").offset(x:0, y:23).font(.system(size: 12, weight: .semibold))
                            }
                            FSpace
                    //Pronouns
                            ZStack{
                                folder
                                Image(.pronouns).resizable().frame(width: 50, height: 50).offset(x:0, y:-6)
                                    .onTapGesture(perform: {dismissKeyboard(); falseFolders(); Pronouns = true})
                                Text("Pronouns").offset(x:0, y:23).font(.system(size: 12, weight: .semibold))
                            }
                            FSpace
